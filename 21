 SELECT
    creature_template.entry,
    creature_template.name,
    creature_template.subname,
    creature_template.Iconame,
    creature_template.type_flags,
    creature_template.type_flags2,
    creature_template.type,
    creature_template.family,
    creature_template.rank,
    creature_template.KillCredit1,
    creature_template.KillCredit2,
    creature_template.PetSpellDataId,
    creature_template.modelid1,
    creature_template.modelid2,
    creature_template.modelid3,
    creature_template.modelid4,
    creature_template.Health_mod,
    creature_template.Mana_mod,
    creature_template.RacialLeader,
    creature_template.movementId,
    creature_template.exp_unk,
    creature_template.WDBVerified,
    creature_template2.gossip_menu_id,
    creature_template2.minlevel,
    creature_template2.maxlevel,
    creature_template2.faction_A,
    creature_template2.faction_H,
    creature_template2.baseattacktime,
    creature_template2.rangeattacktime,
    creature_template2.npcflag,
    creature_template2.speed_walk,
    creature_template2.speed_run
    creature_template2.unit_class,
    creature_template2.unit_flags,
    creature_template2.unit_flags2,
    creature_template2.dynamicflags,
    creature_template2.VehicleId,
    creature_template2.HoverHeight
FROM creature_template
    LEFT OUTER JOIN creature_template2
      ON creature_template.entry = creature_template2.entry
